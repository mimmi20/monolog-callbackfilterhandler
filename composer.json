{
  "name": "mimmi20/monolog-callbackfilterhandler",
  "description": "Monolog handler wrapper that filters records based on a list of callback functions",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Laurent Laville",
      "homepage": "https://github.com/llaville"
    },
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/monolog-callbackfilterhandler",
  "support": {
    "issues": "https://github.com/mimmi20/monolog-callbackfilterhandler/issues",
    "source": "https://github.com/mimmi20/monolog-callbackfilterhandler"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "monolog/monolog": "^3.6.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.11 || ^0.28.1",
    "mimmi20/coding-standard": "^5.2.33",
    "nikic/php-parser": "^4.19.1 || ^5.0.2",
    "phpstan/extension-installer": "^1.4.1",
    "phpstan/phpstan": "^1.11.4",
    "phpstan/phpstan-deprecation-rules": "^1.2.0",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpunit": "^10.5.20",
    "psr/log": "^3.0.0",
    "rector/rector": "^1.1.0",
    "symplify/phpstan-rules": "^13.0.0",
    "tomasvotruba/cognitive-complexity": "^0.2.3",
    "tomasvotruba/type-coverage": "^0.3.0",
    "tomasvotruba/unused-public": "^0.3.9"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\Monolog\\Handler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20\\Monolog\\Handler\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  }
}

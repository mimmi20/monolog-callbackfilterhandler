{
  "name": "mimmi20/monolog-callbackfilterhandler",
  "description": "Monolog handler wrapper that filters records based on a list of callback functions",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Laurent Laville",
      "homepage": "https://github.com/llaville"
    },
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/monolog-callbackfilterhandler",
  "support": {
    "source": "https://github.com/mimmi20/monolog-callbackfilterhandler",
    "issues": "https://github.com/mimmi20/monolog-callbackfilterhandler/issues"
  },
  "require": {
    "php": "^8.1.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "monolog/monolog": "^3.5.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.11",
    "mimmi20/coding-standard": "^5.2.19",
    "nikic/php-parser": "^v4.19.1",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.66",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.16",
    "phpunit/phpunit": "^10.5.16",
    "psr/log": "^3.0.0",
    "rector/rector": "^1.0.3"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\Monolog\\Handler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20\\Monolog\\Handler\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  }
}
